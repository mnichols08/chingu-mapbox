{
  "name": "chingu-mapbox",
  "version": "1.0.0",
  "description": "Mapbox for Chingu Assignment",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass src/sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "build:css": "npm-run-all compile:sass concat:css",
    "dev": "webpack --mode development",
    "webpack-build": "webpack --mode production",
    "build": "npm-run-all build:css webpack-build",
    "webpack": "webpack-dev-server --mode development --open",
    "watch:sass": "node-sass src/sass/main.scss css/style.css -w",
    "start": "npm-run-all --parallel watch:sass webpack"
  },
  "author": "Mikey Nichols",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "mapbox-gl": "^1.2.1"
  }
}
